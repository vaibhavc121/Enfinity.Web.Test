correct code-
 public static void ClearAndProvide2(By locator, string value)
        {
            var element = WaitForElement(locator);          
            Actions actions = new Actions(driver);            
            actions.MoveToElement(element)
                   .Click()
                   .DoubleClick()                  
                   .SendKeys(value)                  
                   .Build()
                   .Perform();
            Thread.Sleep(1000);            
        }